function run(){

  sys.addpack("stm32","STM32","Создание прошивок под микроконтроллеры STM32","hws");


  sys.setcompiler("stm32","STM32_GCC");

  sys.addproject("stm32","STM32F407","STM32F407","Проект прошивки под STM32F407","elf","make_STM32.dll");
  sys.addproject("stm32","STM32F103","STM32F103","Проект прошивки под STM32F103","elf","make_STM32.dll");

}
